<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewBag.LayoutData.PageTitle</title>
    @Html.Raw(ViewBag.LayoutData.RobotsMeta)

    <link href="~/lib/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <link rel="Stylesheet" type="text/css" href="/archive/js/Smooth-Div-Scroll/css/smoothDivScroll.css" />
    <environment names="Development">
        <link rel="stylesheet" href="~/archive/css/site.css" />
    </environment>
    <environment names="Staging,Production">
        <link rel="stylesheet" href="~/archive/css/site.css" asp-append-version="true" />
    </environment>
    @Html.Partial("~/Views/Archive/_Fonts.cshtml")

    <script> var isIE = false;</script>
    <!--[if IE]>
    <script type="text/javascript">
        isIE = true;
     </script>
    <![endif]-->

</head>
<body class="pageTitle_@ViewBag.LayoutData.PageTag pageType_@ViewBag.LayoutData.PageType">
    <div class="container body-content">
        @RenderBody()
        <footer>
            <p class="copyright">@ViewBag.LayoutData.Copyright</p>
        </footer>
    </div>
    @Html.Partial("~/Views/Portfolio/_Analytics.cshtml")

    <environment names="Development">
        <script src="~/lib/jquery/dist/jquery.js"></script>
        <script src="~/archive/js/carousel.js" asp-append-version="true"></script>
    </environment>
    <environment names="Staging,Production">
        <script src="https://ajax.aspnetcdn.com/ajax/jquery/jquery-2.2.0.min.js"
                asp-fallback-src="~/lib/jquery/dist/jquery.min.js"
                asp-fallback-test="window.jQuery">
        </script>
        <script src="~/archive/js/carousel.js" asp-append-version="true"></script>
    </environment>
    @RenderSection("scripts", required: false)
    <script src="/archive/js/Smooth-Div-Scroll/js/jquery-ui-1.8.23.custom.min.js" type="text/javascript"></script>
    <script src="/archive/js/Smooth-Div-Scroll/js/jquery.mousewheel.min.js" type="text/javascript"></script>
    <script src="/archive/js/Smooth-Div-Scroll/js/jquery.kinetic.js" type="text/javascript"></script>
    <script src="/archive/js/Smooth-Div-Scroll/js/jquery.smoothdivscroll-1.3.js" type="text/javascript"></script>

    <script type="text/javascript" src="/archive/js/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="/archive/js/jquery.hoverIntent.minified.js"></script>
    <script type="text/javascript" src="/archive/js/jquery.naviDropDown.1.0.js"></script>

    <script type="text/javascript" src="/archive/js/carousel.js"></script>
</body>
</html>
